{
  "name": "RushQueue",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "db:generate": "kysely-codegen --dialect mysql --numeric-parser number --log-level debug --env-file .env --out-file ./src/db/db.d.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/workers/seatSelection.worker.ts"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.4",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.65.0",
    "chalk": "4",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.8",
    "kysely-codegen": "^0.19.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "stripe": "^20.0.0"
  }
}
